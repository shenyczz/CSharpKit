{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core App (tapp)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "app_build",
            "program": "${workspaceFolder}/bin/app/tapp//Debug/netcoreapp3.1/tapp.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core UnitTest (uts.csk)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "uts_build",
            "program": "${workspaceFolder}/bin/uts/Debug/netcoreapp3.1/uts.csk.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },

        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}